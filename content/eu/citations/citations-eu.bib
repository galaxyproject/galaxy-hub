 shorttitle = {{LAMA2}-{CMD}},
 title = {{LAMA2}-{CMD}: establishment of a new gene therapy strategy using an in vitro model},
 url = {https://repositorio.ul.pt/handle/10451/62797},
 urldate = {2024-11-17},
 year = {2024}
}

