 number = {7},
 pages = {NULL--NULL},
 shorttitle = {Paedomorphic adaptations in a new {Heterostigma} species},
 title = {Paedomorphic adaptations in a new {Heterostigma} species: a novel strategy for ascidians to live in soft-bottom habitats},
 url = {https://www.publish.csiro.au/is/IS24103},
 urldate = {2025-07-12},
