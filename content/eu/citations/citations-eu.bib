 doi = {10.1016/j.dib.2024.110961},
 issn = {2352-3409},
 journal = {Data in Brief},
 keywords = {{\textgreater}UseGalaxy.eu, Human fecal sample, Illumina, Next-generating sequencing, Oxford Nanopore Technologies},
 language = {eng},
