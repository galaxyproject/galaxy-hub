 pages = {957},
 title = {A {Comparative} {Analysis} of {Mitogenomes} in {Species} of the {Tapinoma} nigerrimum {Complex} and {Other} {Species} of the {Genus} {Tapinoma} ({Formicidae}, {Dolichoderinae})},
 url = {http://europepmc.org/abstract/MED/39769559},
 volume = {15},
