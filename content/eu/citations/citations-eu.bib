 title = {Tailored {Multiplex} {Real}-{Time} {RT}-{PCR} with {Species}-{Specific} {Internal} {Positive} {Controls} for {Detecting} {SARS}-{CoV}-2 in {Canine} and {Feline} {Clinical} {Samples}},
 url = {https://www.mdpi.com/2076-2615/13/4/602},
