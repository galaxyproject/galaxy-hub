 language = {en},
 month = {September},
 number = {1},
 pages = {856},
 title = {Comprehensive profiling of ribo-seq detected small sequences in yeast reveals robust conservation patterns and their potential mechanisms of origin},
 url = {https://doi.org/10.1186/s12864-025-12064-0},
