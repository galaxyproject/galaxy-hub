 language = {eng},
 month = {November},
 number = {4},
 pages = {e70563},
 pmcid = {PMC12659883},
 pmid = {41308173},
 shorttitle = {The auxin gatekeepers},
 title = {The auxin gatekeepers: {Evolution} and diversification of the {YUCCA} family},
 url = {https://europepmc.org/articles/PMC12659883},
 urldate = {2025-12-26},
