 doi = {10.3390/microorganisms11051226},
 issn = {2076-2607},
 journal = {Microorganisms},
 keywords = {\textit{Neisseria gonorrhoeae}, {\textgreater}UseGalaxy.eu, G12302 genogroup, NG-MAST, azithromycin resistance, genetic determinants of antimicrobial resistance, phylogenetic analysis},
 language = {en},
