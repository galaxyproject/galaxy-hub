 note = {Publisher: Multidisciplinary Digital Publishing Institute},
 number = {8},
 pages = {583},
 shorttitle = {First {Microbial} {Survey} of a {Submerged} {Petrified} {Forest} in the {Black} {Sea}},
 title = {First {Microbial} {Survey} of a {Submerged} {Petrified} {Forest} in the {Black} {Sea}: {Culture}-{Based} and {Metagenomic} {Insights}},
