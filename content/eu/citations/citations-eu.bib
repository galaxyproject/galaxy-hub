 number = {22},
 pages = {3301},
 pmcid = {PMC12649545},
 pmid = {41302010},
 shorttitle = {Characterization of the {Complete} {Mitogenomes} of {Four} {Dacinae} {Species} ({Diptera}},
 title = {Characterization of the {Complete} {Mitogenomes} of {Four} {Dacinae} {Species} ({Diptera}: {Tephritidae}) with {Phylogenetic} {Analysis}},
