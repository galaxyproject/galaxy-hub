 doi = {10.1038/s41418-022-00992-3},
 issn = {1476-5403},
 journal = {Cell Death \& Differentiation},
 keywords = {{\textgreater}UseGalaxy.eu, Epigenetics, Inflammasome, Long Noncoding, RNA},
 language = {en},
 month = {October},
 note = {Number: 10
Publisher: Nature Publishing Group},
 number = {10},
 pages = {2009--2023},
 title = {Ezh2 competes with p53 to license {lncRNA} {Neat1} transcription for inflammasome activation},
 url = {https://www.nature.com/articles/s41418-022-00992-3},
 urldate = {2022-12-03},
