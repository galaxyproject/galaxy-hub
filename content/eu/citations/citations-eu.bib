 note = {Number: 1
Publisher: Nature Publishing Group},
 number = {1},
 pages = {4674},
 title = {A translational regulator {MHZ9} modulates ethylene signaling in rice},
 url = {https://www.nature.com/articles/s41467-023-40429-0},
 urldate = {2023-11-18},
 volume = {14},
