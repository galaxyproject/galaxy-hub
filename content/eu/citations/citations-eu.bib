 shorttitle = {Molecular docking study of aptamer-based biosensor for detection of {Shiga} {Toxin} ({Stx}) protein from {Escherichia} coli {O157}},
 title = {Molecular docking study of aptamer-based biosensor for detection of {Shiga} {Toxin} ({Stx}) protein from {Escherichia} coli {O157}:{H7}},
