 doi = {10.52825/cordi.v1i.387},
 issn = {2941-296X},
 journal = {Proceedings of the Conference on Research Data Infrastructure},
 keywords = {{\textgreater}UseGalaxy.eu, AAI, Cloud Computing, Federated Research Infrastructure, Training, Virtual Research Environments},
 language = {en},
 month = {September},
 title = {Embedding the de.{NBI} {Cloud} in the {National} {Research} {Data} {Infrastructure} {Activities}},
 url = {https://www.tib-op.org/ojs/index.php/CoRDI/article/view/387},
