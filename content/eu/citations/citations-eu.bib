 shorttitle = {An emerging {Platynota} sp. ({Lepidoptera}},
 title = {An emerging {Platynota} sp. ({Lepidoptera}: {Tortricidae}) infesting blueberry ({Vaccinium} corymbosum) in the central coast of {Peru}},
 url = {https://www.frontiersin.org/journals/insect-science/articles/10.3389/finsc.2025.1593907/full},
 urldate = {2025-07-12},
