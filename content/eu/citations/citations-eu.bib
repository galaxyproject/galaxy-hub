 title = {Degradation of {Non}-coding {RNAs} {Promotes} {Recycling} of {Termination} {Factors} at {Sites} of {Transcription}},
 url = {http://www.sciencedirect.com/science/article/pii/S2211124720309232},
 urldate = {2020-08-13},
