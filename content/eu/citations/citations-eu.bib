 doi = {10.1523/JNEUROSCI.1544-23.2024},
 issn = {0270-6474, 1529-2401},
 journal = {Journal of Neuroscience},
 keywords = {{\textgreater}UseGalaxy.eu, Hippocampus, Intellectual Disability, Jumonji Domain-Containing Histone Demethylases, KDM5B, Memory Consolidation, Memory, Long-Term, chromatin, hippocampus, histone lysine demethylase, learning, memory, mouse},
 language = {en},
