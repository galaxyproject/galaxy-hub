 shorttitle = {Genome-{Based} {Retrospective} {Analysis} of a {Providencia} stuartii {Outbreak} in {Rome}, {Italy}},
 title = {Genome-{Based} {Retrospective} {Analysis} of a {Providencia} stuartii {Outbreak} in {Rome}, {Italy}: {Broad} {Spectrum} {IncC} {Plasmids} {Spread} the {NDM} {Carbapenemase} within the {Hospital}},
