 keywords = {{\textgreater}UseGalaxy.eu, Drug Resistance, Neoplasm, Gene Expression Regulation, Neoplastic, MicroRNAs, Paclitaxel, Triple Negative Breast Neoplasms},
 language = {eng},
 month = {December},
 number = {24},
 pages = {13630},
 title = {Use of {Gain}-of-{Function} {Screening} to {Identify} {miRNAs} {Involved} in {Paclitaxel} {Resistance} in {Triple}-{Negative} {Breast} {Cancer}},
