 pages = {2489},
 title = {Evolutionary {Comparisons} of {Chelonid} {Alphaherpesvirus} 5 ({ChHV5}) {Genomes} from {Fibropapillomatosis}-{Afflicted} {Green} ({Chelonia} mydas), {Olive} {Ridley} ({Lepidochelys} olivacea) and {Kemp}'s {Ridley} ({Lepidochelys} kempii) {Sea} {Turtles}},
