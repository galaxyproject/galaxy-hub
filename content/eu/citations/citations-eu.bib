 issn = {0278-0372},
 journal = {Journal of Crustacean Biology},
 keywords = {{\textgreater}UseGalaxy.eu, Ecology},
 month = {March},
 number = {1},
 pages = {ruae001},
 shorttitle = {Genetic variability of {New} {Caledonian} {Boeckella} {De} {Guerne} \& {Richard}, 1889 ({Copepoda}},
 title = {Genetic variability of {New} {Caledonian} {Boeckella} {De} {Guerne} \& {Richard}, 1889 ({Copepoda}: {Calanoida}), with the description of a new species},
 url = {https://doi.org/10.1093/jcbiol/ruae001},
