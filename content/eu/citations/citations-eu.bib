 language = {en},
 month = {December},
 note = {Publisher: IOP Publishing},
 number = {1},
 pages = {012006},
 title = {Investigating the patterns of simple sequence repeats ({SSRs}) inferred from the whole genome of a critically endangered species {Cassine} koordersii kosterm},
 url = {https://dx.doi.org/10.1088/1755-1315/1271/1/012006},
