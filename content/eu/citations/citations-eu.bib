Publisher: Multidisciplinary Digital Publishing Institute},
 number = {3},
 pages = {436},
 title = {Pilot {Study} on the {Action} of {Thymus} vulgaris {Essential} {Oil} in {Treating} the {Most} {Common} {Bacterial} {Contaminants} and {Salmonella} enterica subsp. enterica {Serovar} {Derby} in {Poultry} {Litter}},
