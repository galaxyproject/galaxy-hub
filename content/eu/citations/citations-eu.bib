 issn = {2076-2607},
 journal = {Microorganisms},
 keywords = {{\textgreater}UseGalaxy.eu, antimicrobial resistance, bacterial endophytes, essential oil, microbiome, plant growth-promoting bacteria},
 language = {en},
 month = {May},
 number = {5},
 pages = {919},
 title = {Genomic {Analysis} of {Endophytic} {Bacillus}-{Related} {Strains} {Isolated} from the {Medicinal} {Plant} {Origanum} vulgare {L}. {Revealed} the {Presence} of {Metabolic} {Pathways} {Involved} in the {Biosynthesis} of {Bioactive} {Compounds}},
 url = {https://www.mdpi.com/2076-2607/10/5/919},
