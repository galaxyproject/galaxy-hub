 title = {Characterization of a highly thermostable recombinant xylanase from \textit{{Anoxybacillu}}s \textit{ayderensis}},
 url = {https://www.sciencedirect.com/science/article/pii/S1046592824000500},
 urldate = {2024-05-17},
 volume = {219},
 year = {2024}
