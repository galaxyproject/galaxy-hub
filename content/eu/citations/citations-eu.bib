}

@article{vila_taboada_erga-bge_2026,
 abstract = {The reference genome of
Erebia palarica
(Chapman’s ringlet; Lepidoptera: Nymphalidae, Satyrinae) provides valuable insights into evolutionary and conservation genomics. On one hand, it paves the way to unravel the speciation processes, reproductive barriers, and potential hybridisation between
E. palarica
and its closely related sister species,
Erebia meolans
