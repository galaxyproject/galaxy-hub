 number = {n/a},
 pages = {e00567},
 title = {C13-{Deacylase} from {Nocardioides} albus {Resp}. {Streptomyces} sp., {Catalyzing} {Side} {Chain} {Hydrolysis} on the {Taxane} {Core}},
 url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/cctc.202500567},
