 month = {May},
 number = {1},
 pages = {48},
 title = {Catecholamine treatment induces reversible heart injury and cardiomyocyte gene expression},
 url = {https://doi.org/10.1186/s40635-024-00632-9},
 urldate = {2024-05-13},
 volume = {12},
