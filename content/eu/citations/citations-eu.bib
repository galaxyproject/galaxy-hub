 journal = {Environmental Microbiology Reports},
 keywords = {{\textgreater}UseGalaxy.eu, Bacteriophages, Drinking Water, Genome, Viral, High-Throughput Nucleotide Sequencing, Metagenomics, Sweden, Virome, Viruses, Water Microbiology, Water Purification, drinking water treatment plant, metagenomics, virome},
