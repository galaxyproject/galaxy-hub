 keywords = {\textit{Rosenblumvirus}, \textit{Staphylococcus} phage, {\textgreater}UseGalaxy.eu, biocontrol, host range},
 language = {en},
 month = {March},
 note = {Number: 3
Publisher: Multidisciplinary Digital Publishing Institute},
 number = {3},
 pages = {664},
 title = {Characterization of {Newly} {Isolated} {Rosenblumvirus} {Phage} {Infecting} {Staphylococcus} aureus from {Different} {Sources}},
