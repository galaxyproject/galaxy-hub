 issn = {2076-0817},
 journal = {Pathogens},
 keywords = {{\textgreater}UseGalaxy.eu, CAZ-AVI, Illumina, MEM-VAR, MLST, cefiderocol, fosfomycin, polymyxin, whole genome sequencing},
 language = {en},
