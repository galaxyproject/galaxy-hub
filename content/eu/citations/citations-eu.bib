 language = {en},
 month = {March},
 note = {Publisher: Nature Publishing Group},
 number = {1},
 pages = {5741},
 title = {Planctoellipticum variicoloris gen. nov., sp. nov., a novel member of the family {Planctomycetaceae} isolated from wastewater of the aeration lagoon of a sugar processing plant in {Northern} {Germany}},
