 title = {{ATP}-{Gated} {P2X7}-{Ion} {Channel} on {Kidney}-{Resident} {Natural} {Killer} {T} {Cells} and {Memory} {T} {Cells} in {Intrarenal} {Inflammation}},
 url = {https://journals.lww.com/jasn/fulltext/9900/atp_gated_p2x7_ion_channel_on_kidney_resident.489.aspx},
