 keywords = {{\textgreater}UseGalaxy.eu, {\textgreater}UseGalaxy.org, {\textgreater}UseGalaxy.org.au, Antimicrobial resistance, Chromosome, ESKAPE pathogens, Mobile genetic element, Mycobacteria, Phage, Plasmid, Virulence},
 language = {en},
 pages = {227--246},
 publisher = {Springer US},
 series = {Methods in {Molecular} {Biology}},
 title = {Genomic {Epidemiological} {Analysis} of {Antimicrobial}-{Resistant} {Bacteria} with {Nanopore} {Sequencing}},
