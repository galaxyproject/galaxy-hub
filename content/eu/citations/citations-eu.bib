 note = {Publisher: Nature Publishing Group},
 number = {1},
 pages = {1939},
 title = {Maternal {PRDM10} activates essential genes for oocyte-to-embryo transition},
 url = {https://www.nature.com/articles/s41467-025-56991-8},
 urldate = {2025-02-27},
 volume = {16},
