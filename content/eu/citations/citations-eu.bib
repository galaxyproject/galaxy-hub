 pages = {e00368--23},
 title = {Multiplicity of {blaKPC} {Genes} and {pKpQIL} {Plasmid} {Plasticity} in the {Development} of {Ceftazidime}-{Avibactam} and {Meropenem} {Coresistance} in {Klebsiella} pneumoniae {Sequence} {Type} 307},
