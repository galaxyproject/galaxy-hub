 title = {Azithromycin {Susceptibility} {Testing} and {Molecular} {Investigation} of {Neisseria} gonorrhoeae {Isolates} {Collected} in {Russia}, 2020–2021},
 url = {https://www.mdpi.com/2079-6382/12/1/170},
