 note = {Publisher: Nature Publishing Group},
 number = {1},
 pages = {595},
 shorttitle = {De novo transcriptomes of cave and surface isopod crustaceans},
 title = {De novo transcriptomes of cave and surface isopod crustaceans: insights from 11 species across three suborders},
 url = {https://www.nature.com/articles/s41597-024-03393-y},
 urldate = {2024-06-08},
 volume = {11},
 year = {2024}
}

