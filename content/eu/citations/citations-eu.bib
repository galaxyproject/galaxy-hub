 pages = {918},
 title = {Genomics and {Antimicrobial} {Susceptibility} of {Clinical} {Pseudomonas} aeruginosa {Isolates} from {Hospitals} in {Brazil}},
 url = {https://www.mdpi.com/2076-0817/12/7/918},
