 pages = {389--398},
 title = {Genome sequence analysis and characterization of {Bacillus} altitudinis {B12}, a polylactic acid- and keratin-degrading bacterium},
 url = {https://doi.org/10.1007/s00438-022-01989-w},
 urldate = {2023-10-28},
 volume = {298},
