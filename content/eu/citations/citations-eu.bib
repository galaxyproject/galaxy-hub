 journal = {Antibiotics},
 keywords = {\textit{Neisseria gonorrhoeae}, \textit{mtrR} and \textit{mtrD} alleles, {\textgreater}UseGalaxy.eu, azithromycin resistance, efflux pump, resistance determinants},
 language = {en},
 month = {January},
