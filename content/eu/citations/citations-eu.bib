 doi = {10.2139/ssrn.5371268},
 keywords = {{\textgreater}UseGalaxy.eu, Bacillus cereus, DALY, Listeria monocytogenes, Monte Carlo, domestic practices, food safety, leguminous},
 language = {en},
 month = {July},
 publisher = {Social Science Research Network},
 title = {Microbial {Risk} and {Health} {Burden} {Associated} with the {Domestic} {Preparation} of {Lentils} in {France} and {Hungary}},
 type = {{SSRN} {Scholarly} {Paper}},
