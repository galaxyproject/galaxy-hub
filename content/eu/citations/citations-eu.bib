 number = {29},
 pages = {5290--5304},
 pmid = {37369586},
 title = {The {Amyloid} {Precursor} {Protein} {Regulates} {Synaptic} {Transmission} at {Medial} {Perforant} {Path} {Synapses}},
 url = {https://www.jneurosci.org/content/43/29/5290},
