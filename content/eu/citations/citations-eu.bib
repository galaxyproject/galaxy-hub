 number = {1},
 pages = {163},
 pmcid = {PMC11767609},
 pmid = {39858930},
 title = {Isolation and {Characterization} of {Colistin}-{Resistant} {Enterobacteriaceae} from {Foods} in {Two} {Italian} {Regions} in the {South} of {Italy}},
 url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC11767609/},
 urldate = {2025-02-16},
