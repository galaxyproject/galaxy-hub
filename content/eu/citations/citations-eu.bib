 title = {Whole-{Genome} {Sequencing} and {Comparative} {Genomic} {Analysis} of {Antimicrobial} {Producing} {Streptococcus} lutetiensis from the {Rumen}},
 url = {http://europepmc.org/abstract/MED/35336126},
 volume = {10},
