 number = {36},
 pages = {e202402780},
 title = {In {Silico} {Prediction} of {Antibacterial} {Activity} of {Quinolone} {Derivatives}},
 url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/slct.202402780},
 urldate = {2024-10-10},
 volume = {9},
