 keywords = {{\textgreater}UseGalaxy.eu, Campylobacter, Flagella, Fletchervirus, Phage, Receptor binding protein, flagellotropic phage, phage receptor, phage resistance},
 language = {English},
 month = {February},
 note = {Publisher: Frontiers},
 title = {A hybrid receptor binding protein enables phage {F341} infection of {Campylobacter} by binding to flagella and lipooligosaccharides},
 url = {https://www.frontiersin.org/journals/microbiology/articles/10.3389/fmicb.2024.1358909/full},
