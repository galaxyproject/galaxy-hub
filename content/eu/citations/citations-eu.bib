 language = {en},
 month = {October},
 note = {Publisher: Nature Publishing Group},
 number = {1},
 pages = {9209},
 title = {Suppressive cancer nonstop extension mutations increase {C}-terminal hydrophobicity and disrupt evolutionarily conserved amino acid patterns},
 url = {https://www.nature.com/articles/s41467-024-52779-4},
 urldate = {2024-10-27},
