 note = {Number: 5
Publisher: Multidisciplinary Digital Publishing Institute},
 number = {5},
 pages = {486},
 shorttitle = {Unusual {Genetic} {Diversity} {Within} {Thereuopoda} clunifera ({Wood}, 1862) ({Chilopoda}},
 title = {Unusual {Genetic} {Diversity} {Within} {Thereuopoda} clunifera ({Wood}, 1862) ({Chilopoda}: {Scutigeromorpha}) {Revealed} by {Phylogeny} and {Divergence} {Times} {Using} {Mitochondrial} {Genomes}},
