 language = {eng},
 month = {October},
 number = {1},
 pages = {37288},
 pmcid = {PMC12552502},
 pmid = {41136472},
 title = {Dissecting {lncRNA}-{mRNA} regulatory network in type 2 diabetes as the risk factor of pancreatic cancer},
