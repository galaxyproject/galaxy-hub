 note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/nph.19686},
 number = {n/a},
 title = {An {RRM} domain protein {SOE} suppresses transgene silencing in rice},
 url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/nph.19686},
 urldate = {2024-05-17},
