 language = {en},
 month = {September},
 pages = {298},
 title = {{ERGA}-{BGE} reference genome of {Hanak}'s bat ({Pipistrellus} hanaki), an {IUCN} {Vulnerable} species restricted to forest-like biotopes},
 url = {https://open-research-europe.ec.europa.eu/articles/5-298/v1},
