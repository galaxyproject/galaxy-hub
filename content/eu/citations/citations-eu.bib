@article{sun_first_2021,
 abstract = {This study reports the first complete mitochondrial genome of \textit{Antheraea pernyi} Guérin-Méneville 1855 (Lepidoptera: Saturniidae) strain Dingzhou\_1, a silkworm resource serving silk production in North Korea. The mitochondrial genome is circular with 15,573 bp in length encoding 37 typical mitochondrial genes (13 protein-coding genes, two ribosomal \textit{RNA} genes, and 22 transfer \textit{RNA} genes). The 553-bp A + T-rich region harbors a repeat region composed of 6 ∼ 38 bp tandem repeat units, as found in other known inbred strains from Chinese populations. The phylogenetic analysis clustered Dingzhou\_1 from North Korea together with Chinese Liaoning population, suggesting that oak silkworm in North Korea might be introduced from her neighbor China Liaoning.},
