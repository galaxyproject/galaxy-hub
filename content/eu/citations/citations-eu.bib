 language = {en},
 month = {October},
 number = {n/a},
 pages = {1900147},
 title = {Scalable {Data} {Analysis} in {Proteomics} and {Metabolomics} {Using} {BioContainers} and {Workflows} {Engines}},
 url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/pmic.201900147},
 urldate = {2019-11-27},
 volume = {n/a},
