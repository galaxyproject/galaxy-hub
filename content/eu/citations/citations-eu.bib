 title = {Single-{Cell} {Protein} and {Transcriptional} {Characterization} of {Epiretinal} {Membranes} {From} {Patients} {With} {Proliferative} {Vitreoretinopathy}},
 url = {http://europepmc.org/abstract/MED/35579905},
 volume = {63},
 year = {2022}
}

