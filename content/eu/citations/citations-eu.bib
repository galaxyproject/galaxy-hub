 pages = {91--100},
 title = {Characterization of {Verona} {Integron}-{Encoded} {Metallo}-β-{Lactamase}-{Type} {Carbapenemase}-{Producing} {Escherichia} coli {Isolates} {Collected} over a 16-{Year} {Period} in {Bolzano} ({Northern} {Italy})},
