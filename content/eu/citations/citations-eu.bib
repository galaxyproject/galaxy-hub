 shorttitle = {Comprehensive {Genomic} and {Phenotypic} {Characterization} of \&lt;i\&gt;{Escherichia} coli\&lt;/i\&gt; {O78}},
 title = {Comprehensive {Genomic} and {Phenotypic} {Characterization} of \&lt;i\&gt;{Escherichia} coli\&lt;/i\&gt; {O78}:{H9} {Strain} {HPVN24} {Isolated} from {Diarrheic} {Poultry} in {Vietnam}},
