 title = {Detection of {KPC}-216, a {Novel} {KPC}-3 {Variant}, in a {Clinical} {Isolate} of {Klebsiella} pneumoniae {ST101} {Co}-{Resistant} to {Ceftazidime}-{Avibactam} and {Cefiderocol}},
