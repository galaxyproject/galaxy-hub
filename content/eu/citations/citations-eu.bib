 shorttitle = {Wall-{Associated} {Kinase} ({WAK}) and {WAK}-like {Kinase} {Gene} {Family} in {Sugar} {Beet}},
 title = {Wall-{Associated} {Kinase} ({WAK}) and {WAK}-like {Kinase} {Gene} {Family} in {Sugar} {Beet}: {Genome}-{Wide} {Characterization} and {In} {Silico} {Expression} {Analysis} in {Response} to {Beet} {Cyst} {Nematode} ({Heterodera} schachtii {Schmidt}) {Infection}},
