 issn = {2045-2322},
 journal = {Scientific Reports},
 keywords = {+Methods, +UsePublic, {\textgreater}UseGalaxy.eu, Gene Expression Regulation, Neoplastic},
 language = {en},
 month = {October},
 note = {Number: 1
Publisher: Nature Publishing Group},
 number = {1},
 pages = {17022},
 title = {Transcriptional characterization of conjunctival melanoma identifies the cellular tumor microenvironment and prognostic gene signatures},
