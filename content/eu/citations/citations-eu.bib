 journal = {International Journal of Molecular Sciences},
 keywords = {\textit{Neisseria gonorrhoeae}, {\textgreater}UseGalaxy.eu, Gonorrhea, Multilocus Sequence Typing, Neisseria gonorrhoeae, Phylogeny, antimicrobial resistance, genogroups, informative polymorphisms, multilocus sequence typing, oligonucleotide microarray, phylogenetic analysis},
 language = {en},
