 month = {October},
 pages = {107976},
 title = {Structure and {Energetics} of {Serum} {Protein} {Complex} of {Tea} {Adulterant} {Dye} {Bismarck} {Brown} {Y} using {Experimental} and {Computational} {Methods}},
