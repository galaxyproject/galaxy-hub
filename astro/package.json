    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@vueuse/core": "^14.1.0",
    "astro": "^5.16.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "glob": "^13.0.0",
    "gray-matter": "^4.0.3",
